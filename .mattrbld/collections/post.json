{
  "allowedTypes": [],
  "defaultFilename": "datetime",
  "dir": "/posts",
  "disableComments": false,
  "disablePreview": false,
  "draftByDefault": false,
  "schemas": [
    "/.mattrbld/schemas/post.json"
  ],
  "linkable": true,
  "maxSize": null,
  "permissions": {
    "everybody": [
      "createContent",
      "deleteContent",
      "editContent"
    ]
  },
  "type": "md",
  "urlTemplate": "/posts/:date[year]-:date[month]-:date[day]-:title/"
}